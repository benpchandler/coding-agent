{
    "openai_api_key": "${OPENAI_API_KEY}",
    "model": "gpt-4-turbo-preview",
    "temperature": 0.7,
    "max_tokens": 4000,
    "agents": {
        "orchestration": {
            "enabled": true,
            "max_retries": 3
        },
        "testing": {
            "enabled": true,
            "test_timeout": 300
        },
        "quality": {
            "enabled": true,
            "min_quality_score": 0.8
        },
        "implementation": {
            "enabled": true,
            "max_implementation_attempts": 3
        }
    },
    "logging": {
        "level": "INFO",
        "format": "%(asctime)s - %(name)s - %(levelname)s - %(message)s"
    }
} 